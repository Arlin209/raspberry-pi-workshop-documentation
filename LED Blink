from machine import Pin
from time import sleep

# Set up the LED pin (GP25 is the onboard LED on Raspberry Pi Pico)
led = Pin(25, Pin.OUT)

# Repeat forever
while True:
    led.value(1)  # Turn LED ON
    sleep(1)      # Wait for 1 second
    led.value(0)  # Turn LED OFF
    sleep(1)      # Wait for 1 second
